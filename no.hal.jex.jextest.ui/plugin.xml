	<extension point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            label="JUnit Test"
            class="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:no.hal.jex.jextest.ui.launch.JextestLaunchShortcut"
            icon="icons/jex.gif"
            modes="run, debug"
            id="no.hal.jex.jextest.launch.shortcut">
        <contextualLaunch>
           <enablement>
              <with variable="selection">
                 <count value="1"/>
                   <iterate>
		                <and>
			                <not><adapt type="org.eclipse.jdt.core.IJavaElement"/></not>
			                <adapt type="no.hal.jex.jextest.ui.launch.JextestJavaElementDelegate">
								<adapt type="org.eclipse.jdt.core.IJavaElement">
									<test property="org.eclipse.jdt.core.isInJavaProject"/>
									<test property="org.eclipse.jdt.core.hasTypeOnClasspath" value="junit.framework.Test"/>
									<test property="org.eclipse.jdt.junit.canLaunchAsJUnit" forcePluginActivation="true"/>
								</adapt>
							</adapt>
						</and>
					</iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <configurationType id="org.eclipse.jdt.junit.launchconfig"/>
         <description description="Debugs a set of JUnit tests" mode="debug"/>
         <description description="Runs a set of JUnit tests" mode="run"/>
      </shortcut>
   </extension>
  
