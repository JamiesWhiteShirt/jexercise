<plugin>

    <extension
            point="org.eclipse.ui.editors">
        <editor
            class="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="jextest"
            icon="icons/jex.gif"
            id="no.hal.jex.jextest.JexTest"
            matchingStrategy="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:org.eclipse.xtext.xbase.ui.editor.JavaEditorInputMatcher"
            name="JexTest Editor">
        </editor>
    </extension>
    
    <extension
          point="org.eclipse.ui.newWizards">
       <category
             id="no.hal.jex.jextest.JexTest"
             name="JExercise">
       </category>
       <wizard
             category="no.hal.jex.jextest.ui"
             class="no.hal.jex.jextest.ui.wizards.NewJextestFileWizard"
             icon="icons/jex.gif"
             id="no.hal.jex.jextest.ui.wizards.NewJextestFileWizard"
             name="Jextest file">
       </wizard>
    </extension>

    <extension
        point="org.eclipse.ui.handlers">
        <handler
            class="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
            commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
            <activeWhen>
                <reference
                    definitionId="no.hal.jex.jextest.JexTest.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
            commandId="no.hal.jex.jextest.JexTest.validate">
         <activeWhen>
            <reference
                    definitionId="no.hal.jex.jextest.JexTest.Editor.opened">
            </reference>
         </activeWhen>
      	</handler>
      	<!-- copy qualified name -->
        <handler
            class="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
            <activeWhen>
				<reference definitionId="no.hal.jex.jextest.JexTest.Editor.opened" />
            </activeWhen>
        </handler>
        <handler
            class="no.hal.jex.jextest.ui.JexTestExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
            <activeWhen>
            	<and>
            		<reference definitionId="no.hal.jex.jextest.JexTest.XtextEditor.opened" />
	                <iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
            </activeWhen>
        </handler>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="no.hal.jex.jextest.JexTest.Editor.opened">
            <and>
                <reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="no.hal.jex.jextest.JexTest" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
        <definition id="no.hal.jex.jextest.JexTest.XtextEditor.opened">
            <and>
                <reference definitionId="isXtextEditorActive"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="no.hal.jex.jextest.JexTest" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
    </extension>
